{"ast":null,"code":"import { mapActions } from \"vuex\";\nexport default {\n  name: \"AddPrf\",\n  data() {\n    return {\n      status: \"\",\n      message: \"\",\n      name: \"\",\n      code: \"#FFFFFF\",\n      active: false,\n      //MODAL\n\n      viewModal: false\n    };\n  },\n  methods: {\n    ...mapActions(\"profile\", [\"setAddedPrf\"]),\n    ...mapActions(\"profile\", [\"setHistoryOptionPrf\"]),\n    wait: function () {\n      setTimeout(() => this.$router.go(), 200);\n    },\n    submit: async function (name, code) {\n      this.status = \"\";\n      this.message = \"\";\n      let profile = {\n        name,\n        code,\n        main: \"no\"\n      };\n      let result = await this.$store.dispatch(\"profile/addItemPrf\", {\n        option: \"profile\",\n        item: profile\n      });\n      if (result.status == \"error\") {\n        this.status = \"error\";\n        this.message = result.message;\n      } else {\n        // success\n        this.setAddedPrf(result.added);\n        this.showModal();\n      }\n    },\n    showModal: function () {\n      this.viewModal = true;\n    },\n    closeModal: function () {\n      this.viewModal = false;\n      this.setAddedPrf(\"\");\n      this.setHistoryOptionPrf(\"Default\");\n    },\n    Add: function () {\n      this.status = \"\";\n      this.message = \"\";\n      this.name = \"\";\n      this.code = \"\";\n      this.viewModal = false;\n      this.setAddedPrf(\"\");\n    },\n    toEdit: function () {\n      this.setHistoryOptionPrf(\"Edit\");\n      this.viewModal = false;\n    },\n    back: function () {\n      setTimeout(() => this.$router.go(), 200);\n    }\n  }\n};","map":{"version":3,"names":["mapActions","name","data","status","message","code","active","viewModal","methods","wait","setTimeout","$router","go","submit","profile","main","result","$store","dispatch","option","item","setAddedPrf","added","showModal","closeModal","setHistoryOptionPrf","Add","toEdit","back"],"sources":["src/views/profile/addPrf.vue"],"sourcesContent":["<template >\r\n  <div id=\"AddPrf\">\r\n    <div class=\"form-content\">\r\n      <form\r\n        @submit.prevent=\"\r\n          submit(name, code)\r\n        \"\r\n      >\r\n        <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label for=\"name\" class=\"name\">Nombre </label>\r\n            <input\r\n              v-model=\"name\"\r\n              type=\"text\"\r\n              id=\"name\"\r\n              name=\"name\"\r\n             \r\n              required\r\n            />\r\n          </div>\r\n         <div class=\"boxform\">\r\n            <label for=\"code\" class=\"code\">Color</label>\r\n            <input\r\n              v-model=\"code\"\r\n              type=\"color\"\r\n              id=\"code\"\r\n              name=\"code\"\r\n              required\r\n            />\r\n          </div>\r\n        \r\n        </div>\r\n\r\n      \r\n        <div class=\"dflx\">\r\n          <p @click=\"wait()\" class=\"btnRegresar\">Regresar</p>\r\n          <button class=\"alta\" type=\"submit\">Guardar</button>\r\n        </div>\r\n\r\n        <div v-if=\"status != ''\" class=\"status_messages\">\r\n          <div v-if=\"status == 'success'\" class=\"msg msg_success\">\r\n            <p>{{ message }}</p>\r\n          </div>\r\n          <div v-if=\"status == 'error'\" class=\"msg msg_error\">\r\n            <p>{{ message }}</p>\r\n          </div>\r\n        </div>\r\n      </form>\r\n\r\n      <div v-if=\"viewModal\" class=\"modal_view\">\r\n        <div id=\"modal_add\">\r\n          <div class=\"body dflx\">\r\n            <h3>Color agregado</h3>\r\n            <img src=\"../../assets/default/add.png\" alt=\"icono alerta\" />\r\n          </div>\r\n\r\n          <div class=\"modal_add_btns dflx\">\r\n            <p @click=\"Add()\" class=\"otro\">Aceptar</p>\r\n            <p @click=\"toEdit()\" class=\"editar\">Editar</p>\r\n            <p @click=\"back()\" class=\"inicio\">Inicio</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapActions } from \"vuex\";\r\nexport default {\r\n  name: \"AddPrf\",\r\n  data() {\r\n    return {\r\n      status: \"\",\r\n      message: \"\",\r\n      name: \"\",\r\n      code:\"#FFFFFF\",\r\n    \r\n      active: false,\r\n    \r\n\r\n      //MODAL\r\n\r\n      viewModal: false,\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapActions(\"profile\", [\"setAddedPrf\"]),\r\n    ...mapActions(\"profile\", [\"setHistoryOptionPrf\"]),\r\n    wait: function () {\r\n      setTimeout(() => this.$router.go(), 200);\r\n    },\r\n    submit: async function (name, code) {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n\r\n        let profile = {\r\n          name,\r\n          code,\r\n          main:\"no\"\r\n        };\r\n\r\n        let result = await this.$store.dispatch(\"profile/addItemPrf\", {\r\n          option: \"profile\",\r\n          item: profile,\r\n        });\r\n\r\n        if (result.status == \"error\") {\r\n          this.status = \"error\";\r\n          this.message = result.message;\r\n        } else {\r\n          // success\r\n          this.setAddedPrf(result.added);\r\n          this.showModal();\r\n        }\r\n   \r\n    },\r\n  \r\n\r\n    showModal: function () {\r\n      this.viewModal = true;\r\n    },\r\n    closeModal: function () {\r\n      this.viewModal = false;\r\n      this.setAddedPrf(\"\");\r\n      this.setHistoryOptionPrf(\"Default\");\r\n    },\r\n    Add: function () {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n      this.name = \"\";\r\n      this.code = \"\";\r\n    \r\n      this.viewModal = false;\r\n      this.setAddedPrf(\"\");\r\n    },\r\n    toEdit: function () {\r\n      this.setHistoryOptionPrf(\"Edit\");\r\n      this.viewModal = false;\r\n    },\r\n    back: function () {\r\n      setTimeout(() => this.$router.go(), 200);\r\n    },\r\n  },\r\n};\r\n</script>\r\