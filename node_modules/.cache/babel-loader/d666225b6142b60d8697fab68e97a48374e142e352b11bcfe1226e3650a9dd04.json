.$store.dispatch(\"social_media/editItemSM\", {\n        option: \"social_media\",\n        item: {\n          id: this.added.id,\n          social_media,\n          link\n        }\n      });\n      if (result.status == \"error\") {\n        this.status = \"error\";\n        this.message = result.message;\n      } else {\n        // success\n        this.showModal();\n      }\n    },\n    delStatus: function () {\n      setTimeout(() => this.delMsgs(), 2000);\n    },\n    delMsgs: function () {\n      this.status = \"\";\n      this.message = \"\";\n    },\n    showModal: function () {\n      this.viewModal = true;\n    },\n    Add: function () {\n      this.status = \"\";\n      this.message = \"\";\n      this.social_media = \"\";\n      this.link = \"\";\n      this.viewModal = false;\n      this.setAddedSM(\"\");\n    },\n    toEdit: function () {\n      this.setHistoryOptionSM(\"Edit\");\n      this.viewModal = false;\n    },\n    back: function () {\n      setTimeout(() => this.$router.go(), 200);\n    }\n  }\n};","map":{"version":3,"names":["mapActions","VueEditor","name","components","data","status","message","social_media","link","files","viewModal","search_value","computed","added","$store","getters","identity","methods","previewFiles","$refs","myFiles","addEtiqueta","value","newarr","item","etiquetas","push","etiqueta","delEtiqueta","index","i","length","wait","setTimeout","$router","go","includesItem","search","normalize","replace","toLowerCase","includes","submit","result","dispatch","option","id","showModal","delStatus","delMsgs","Add","setAddedSM","toEdit","setHistoryOptionSM","back"],"sources":["src/views/social_media/editSM.vue"],"sourcesContent":["<template>\r\n  <div id=\"AddSM\">\r\n    <div class=\"form-content\">\r\n      <form @submit.prevent=\"submit(added.social_media, added.link)\"> \r\n        <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label for=\"titulo\" class=\"titulo\">Red social </label>\r\n  