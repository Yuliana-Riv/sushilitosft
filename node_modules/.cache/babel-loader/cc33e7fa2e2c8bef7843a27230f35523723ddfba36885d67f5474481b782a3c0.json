{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions } from \"vuex\";\nimport AddBnr from \"./addBnr.vue\";\nimport { url, endpointCodes } from \"../../global/index\";\nexport default {\n  name: \"Banner\",\n  components: {\n    AddBnr\n  },\n  data() {\n    return {\n      url: url,\n      endpointCodes,\n      status: \"\",\n      message: \"\",\n      item: \"\",\n      search_value: \"\",\n      updsearchcli: \"\",\n      currentPage1: 1,\n      itemsPerPage1: 12,\n      currentPage2: 1,\n      itemsPerPage2: 12,\n      currentPage3: 1,\n      itemsPerPage3: 12,\n      active: false,\n      viewModal: false,\n      viewModal2: false,\n      itemDel: \"\",\n      radius: false,\n      datab: []\n    };\n  },\n  created() {\n    this.getAllInfoBnr(\"banner\");\n  },\n  updated() {\n    if (this.search_value != this.updsearchcli) {\n      this.updsearchcli = this.search_value;\n      let option = \"banner\";\n      let value = this.search_value;\n      let payload = {\n        option: option,\n        search: value\n      };\n      if (value != \"\") {\n        this.$store.dispatch(\"banner/searchBnr\", payload);\n      } else {\n        this.getAllInfoBnr(\"banner\");\n      }\n    }\n  },\n  computed: {\n    data() {\n      return this.$store.getters[\"banner/data\"];\n    },\n    dataslide() {\n      let data = this.$store.getters[\"banner/data\"];\n      let arr = [];\n      if (data == \"No se encontraron coincidencias\") {\n        return data;\n      }\n      for (let i = 0; i < data.length; i++) {\n        if (data[i].type == \"slide\") {\n          arr.push(data[i]);\n        }\n      }\n      return arr;\n    },\n    databanneruno() {\n      let data = this.$store.getters[\"banner/data\"];\n      let arr = [];\n      if (data == \"No se encontraron coincidencias\") {\n        return data;\n      }\n      for (let i = 0; i < data.length; i++) {\n        if (data[i].type == \"banner-1\") {\n          arr.push(data[i]);\n        }\n      }\n      return arr;\n    },\n    databannerdos() {\n      let data = this.$store.getters[\"banner/data\"];\n      let arr = [];\n      if (data == \"No se encontraron coincidencias\") {\n        return data;\n      }\n      for (let i = 0; i < data.length; i++) {\n        if (data[i].type == \"banner-2\") {\n          arr.push(data[i]);\n        }\n      }\n      return arr;\n    },\n    historyOption() {\n      return this.$store.getters[\"banner/getHistoryOption\"];\n    },\n    //Paginacion\n    allItems1() {\n      return Object.keys(this.dataslide).map(pid => this.dataslide[pid]);\n    },\n    filteredItems1() {\n      let db = this.allItems1.slice((this.currentPage1 - 1) * this.itemsPerPage1, this.currentPage1 * this.itemsPerPage1);\n      this.datab = db;\n      return db;\n    },\n    totalPages1() {\n      let total = Math.ceil(this.allItems1.length / this.itemsPerPage1);\n      if (total < this.currentPage1) {\n        this.currentPage1 = 1;\n      }\n      return total;\n    },\n    //Paginacion\n    allItems2() {\n      return Object.keys(this.databanneruno).map(pid => this.databanneruno[pid]);\n    },\n    filteredItems2() {\n      let db = this.allItems2.slice((this.currentPage2 - 1) * this.itemsPerPage2, this.currentPage2 * this.itemsPerPage2);\n      this.datab = db;\n      return db;\n    },\n    totalPages2() {\n      let total = Math.ceil(this.allItems2.length / this.itemsPerPage2);\n      if (total < this.currentPage2) {\n        this.currentPage2 = 1;\n      }\n      return total;\n    },\n    //Paginacion\n    allItems3() {\n      return Object.keys(this.databannerdos).map(pid => this.databannerdos[pid]);\n    },\n    filteredItems3() {\n      let db = this.allItems3.slice((this.currentPage3 - 1) * this.itemsPerPage3, this.currentPage3 * this.itemsPerPage3);\n      this.datab = db;\n      return db;\n    },\n    totalPages3() {\n      let total = Math.ceil(this.allItems3.length / this.itemsPerPage3);\n      if (total < this.currentPage3) {\n        this.currentPage3 = 1;\n      }\n      return total;\n    }\n  },\n  methods: {\n    ...mapActions(\"banner\", [\"getAllInfoBnr\"]),\n    ...mapActions(\"banner\", [\"setHistoryOptionBnr\"]),\n    ...mapActions(\"banner\", [\"setAddedBnr\"]),\n    ...mapActions(\"main\", [\"setAddedMain\"]),\n    UpItem: function (item, key) {\n      let banners = this.data;\n      let anterior = key - 1;\n      let actual = key;\n      let item_actual = item;\n      let item_anterior = banners[key - 1];\n      let array = [];\n      for (var i = 0; i < banners.length; i++) {\n        if (i == actual) {\n          array.push(item_anterior);\n        } else if (i == anterior) {\n          array.push(item_actual);\n        } else {\n          array.push(banners[i]);\n        }\n      }\n      //console.log('cup')\n      //this.data =array\n      //this.datab = array;\n      this.setAddedBnr(array);\n    },\n    DownItem: function (item, key) {\n      let banners = this.data;\n      let siguiente = key + 1;\n      let actual = key;\n      let item_actual = item;\n      let item_siguiente = banners[key + 1];\n      let array = [];\n      for (var i = 0; i < banners.length; i++) {\n        if (i == actual) {\n          array.push(item_siguiente);\n        } else if (i == siguiente) {\n          array.push(item_actual);\n        } else {\n          array.push(banners[i]);\n        }\n      }\n      //this.datab = array;\n      //console.log('ckr')\n      //console.log(array)\n      this.setAddedBnr(array);\n    },\n    changeView(option, item) {\n      this.item = item;\n      console.log(item);\n      this.setAddedBnr(item);\n      this.setHistoryOptionBnr(option);\n    },\n    activeRadius() {\n      this.radius ? this.radius = false : this.radius = true;\n    },\n    showModal: function (id) {\n      this.itemDel = id;\n      this.viewModal = true;\n    },\n    closeModal: function () {\n      this.viewModal = false;\n      this.itemDel = \"\";\n    },\n    showModal2: function () {\n      this.viewModal = true;\n    },\n    closeModal2: function () {\n      this.viewModal = false;\n    },\n    wait: function () {\n      setTimeout(() => this.$router.go(), 200);\n    },\n    delStatus: function () {\n      setTimeout(() => this.delMsgs(), 2000);\n      setTimeout(() => this.wait(), 500);\n    },\n    delMsgs: function () {\n      this.status = \"\";\n      this.message = \"\";\n    },\n    eliminar: async function () {\n      let id = this.itemDel;\n      this.status = \"\";\n      this.message = \"\";\n      console.log(id);\n      let result = await this.$store.dispatch(\"banner/deleteItemBnr\", {\n        option: \"banner\",\n        id: id\n      });\n      if (result.status == \"error\") {\n        this.message = result.message;\n        this.status = \"error\";\n        this.delStatus();\n      } else {\n        this.status = \"\";\n        this.message = \"\";\n        this.wait();\n      }\n    },\n    search: async function (value) {\n      let option = \"banner\";\n      let payload = {\n        option: option,\n        search: value\n      };\n      if (value != \"\") {\n        this.$store.dispatch(\"banner/searchBnr\", payload);\n      } else {\n        this.getAllInfoBnr(\"banner\");\n      }\n    },\n    actualizarOrden: async function () {\n      let banner = this.data;\n      let result = await this.$store.dispatch(\"banner/editItemBnr\", {\n        option: \"actualizar_orden\",\n        item: {\n          banner\n        }\n      });\n      if (result.status == \"error\") {\n        this.msg = result.message;\n        this.showModalError();\n      } else {\n        // success\n        this.$store.dispatch(\"banner/setHistoryOptionBnr\", {\n          option: \"banner\"\n        });\n        this.getAllInfoBnr(\"banner\");\n        this.viewModal2 = true;\n      }\n    },\n    // botones de paginacion.\n    next1() {\n      if (this.currentPage1 < this.totalPages1) {\n        this.currentPage1++;\n      }\n    },\n    prev1() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    next2() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prev2() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    next3() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prev3() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapActions","AddBnr","url","endpointCodes","name","components","data","status","message","item","search_value","updsearchcli","currentPage1","itemsPerPage1","currentPage2","itemsPerPage2","currentPage3","itemsPerPage3","active","viewModal","viewModal2","itemDel","radius","datab","created","getAllInfoBnr","updated","option","value","payload","search","$store","dispatch","computed","getters","dataslide","arr","i","length","type","push","databanneruno","databannerdos","historyOption","allItems1","Object","keys","map","pid","filteredItems1","db","slice","totalPages1","total","Math","ceil","allItems2","filteredItems2","totalPages2","allItems3","filteredItems3","totalPages3","methods","UpItem","key","banners","anterior","actual","item_actual","item_anterior","array","setAddedBnr","DownItem","siguiente","item_siguiente","changeView","console","log","setHistoryOptionBnr","activeRadius","showModal","id","closeModal","showModal2","closeModal2","wait","setTimeout","$router","go","delStatus","delMsgs","eliminar","result","actualizarOrden","banner","msg","showModalError","next1","prev1","currentPage","next2","totalPages","prev2","next3","prev3"],"sources":["src/views/banner/banner.vue"],"sourcesContent":["<template>\r\n  <div id=\"Categorias\">\r\n    <div v-if=\"historyOption == 'Default'\">\r\n      <!---SLIDERS-->\r\n      <section>\r\n        <h3 class=\"tlpr\">SLIDE PRINCIPAL</h3>\r\n        <div class=\"dflx\">\r\n          <p class=\"noitems\">\r\n            No. de items\r\n            <span v-if=\"dataslide != 'No se encontraron coincidencias'\">{{\r\n              dataslide.length\r\n            }}</span>\r\n            <span v-else>0</span>\r\n          </p>\r\n          <div class=\"select-container\">\r\n            <input\r\n              type=\"checkbox\"\r\n              name=\"select-ch\"\r\n              id=\"select-ch\"\r\n              v-model=\"active\"\r\n              true-value=\"true\"\r\n              false-value=\"false\"\r\n            />\r\n            <label for=\"select-ch\">\r\n              <div id=\"select-items\" :class=\"{ rounded: active }\">\r\n                Items por página: {{ itemsPerPage1 }}\r\n              </div>\r\n            </label>\r\n            <div class=\"options-cont\" @click=\"closeSelect()\">\r\n              <div\r\n                class=\"option-btn\"\r\n                v-bind:value=\"6\"\r\n                @click=\"itemsPerPage1 = '6'\"\r\n              >\r\n                6\r\n              </div>\r\n              <div\r\n                class=\"option-btn\"\r\n                v-bind:value=\"12\"\r\n                @click=\"itemsPerPage1 = '12'\"\r\n              >\r\n                12\r\n              </div>\r\n              <div\r\n                class=\"option-btn\"\r\n                v-bind:value=\"18\"\r\n                @click=\"itemsPerPage1 = '18'\"\r\n              >\r\n                18\r\n              </div>\r\n              <div\r\n                class=\"option-btn\"\r\n                v-bind:value=\"24\"\r\n                @click=\"itemsPerPage1 = '24'\"\r\n              >\r\n                24\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!---\r\n          <div class=\"buscador dflx\">\r\n            <input\r\n              v-model=\"search_value\"\r\n              type=\"search\"\r\n              id=\"input-search\"\r\n              class=\"form-control\"\r\n              :placeholder=\"'Búsqueda'\"\r\n            />\r\n            <div class=\"icon_buscar\">\r\n              <img\r\n                @click=\"search(search_value)\"\r\n                src=\"../../assets/default/Search.png\"\r\n                alt=\"icono buscar\"\r\n              />\r\n            </div>\r\n          </div>--->\r\n        </div>\r\n\r\n        <div class=\"dflx widthbtn\">\r\n          <div @click=\"changeView('Add', 'slide')\" class=\"btn_añadir dflx\">\r\n            <p>AGREGAR</p>\r\n            <img src=\"../../assets/default/add.png\" alt=\"\" />\r\n          </div>\r\n\r\n          <div @click=\"actualizarOrden()\" class=\"btn_añadir act-ord dflx\">\r\n            <p>ACTUALIZAR ORDEN</p>\r\n            <img src=\"../../assets/default/add.png\" alt=\"\" />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"tbl_categorias\">\r\n          <div class=\"tbl_header dflx\">\r\n            <div class=\"tblh tblrad\">\r\n              <p>IMAGEN PC</p>\r\n            </div>\r\n            <div class=\"tblh\">\r\n              <p>IMAGEN MOVIL</p>\r\n            </div>\r\n            <div class=\"tblh\">\r\n              <p>LINK</p>\r\n            </div>\r\n\r\n            <div class=\"tblh2\">\r\n              <p>EDITAR</p>\r\n            </div>\r\n            <div class=\"tblh2 tblh2r\">\r\n              <p>BORRAR</p>\r\n            </div>\r\n          </div>\r\n          <div tbl_content>\r\n            <div\r\n              v-if=\"\r\n                dataslide != 'No se encontraron coincidencias' &&\r\n                dataslide != 'error' &&\r\n                dataslide != ''\r\n              \"\r\n            >\r\n              <div\r\n                class=\"tbl_items dflx\"\r\n                v-for=\"(item, key) in filteredItems1\"\r\n                :key=\"key\"\r\n              >\r\n                <div class=\"tbl_item\">\r\n                  <img\r\n                    :src=\"\r\n                      url  +\r\n                      '/get-banner-image/' +\r\n                      item.image\r\n                    \"\r\n                    alt=\"image\"\r\n                  />\r\n                </div>\r\n                <div class=\"tbl_item\">\r\n                  <img\r\n                    :src=\"\r\n                      url  +\r\n                      '/get-banner-image/' +\r\n                      item.image_mv\r\n                    \"\r\n                    alt=\"image\"\r\n                  />\r\n                </div>\r\n                <div class=\"tbl_item\">\r\n                  <p>{{ item.link }}</p>\r\n                </div>\r\n                <div class=\"icon_eliminar\">\r\n                  <h2 v-if=\"key != 0\" @click=\"UpItem(item, key)\" class=\"cp\">\r\n                    &#9650;\r\n                  </h2>\r\n                  <!--  -->\r\n                  <h2\r\n                    v-if=\"key < data.length - 1\"\r\n                    @click=\"DownItem(item, key)\"\r\n                    class=\"cp\"\r\n                  >\r\n                    &#9660;\r\n                  </h2>\r\n                  <!--  -->\r\n                </div>\r\n\r\n                <div class=\"icon_eliminar\">\r\n                  <img\r\n                    @click=\"showModal(item.id)\"\r\n                    src=\"../../assets/default/i_eliminar.png\"\r\n                    alt=\"icono eliminar\"\r\n                  />\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else>\r\n              <p class=\"snu\">Aun no hay registros.</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div id=\"navigate-buttons\" class=\"mlf\">\r\n          <div class=\"dflx\">\r\n            <button @click=\"prev1\" id=\"btn-prev\" class=\"btn-navigate\">\r\n              <img src=\"../../assets/default/Left.png\" alt=\"icono anterior\" />\r\n            </button>\r\n            <p class=\"num-pages\">{{ currentPage1 }} / {{ totalPages1 }}</p>\r\n            <button @click=\"next1\" id=\"btn-next\" class=\"btn-navigate\">\r\n              <img src=\"../../assets/default/Right.png\" alt=\"icono siguiente\" />\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </section>\r\n\r\n      <!---BANNER 1-->\r\n      <section>\r\n        <h3 class=\"tlpr\">BANNER 1</h3>\r\n        <div class=\"dflx\">\r\n          <p class=\"noitems\">\r\n            No. de items\r\n            <span v-if=\"databanneruno != 'No se encontraron coincidencias'\">{{\r\n              databanneruno.length\r\n            }}</span>\r\n            <span v-else>0</span>\r\n          </p>\r\n          <div class=\"select-container\">\r\n            <input\r\n              type=\"checkbox\"\r\n              name=\"select-ch\"\r\n              id=\"select-ch\"\r\n              v-model=\"active\"\r\n              true-value=\"true\"\r\n              false-value=\"false\"\r\n            />\r\n            <label for=\"select-ch\">\r\n              <div id=\"select-items\" :class=\"{ rounded: active }\">\r\n                Items por página: {{ itemsPerPage2 }}\r\n              </div>\r\n            </label>\r\n            <div class=\"options-cont\" @click=\"closeSelect()\">\r\n              <div\r\n                class=\"option-btn\"\r\n                v-bind:value=\"6\"\r\n                @click=\"itemsPerPage2 = '6'\"\r\n              >\r\n                6\r\n              </div>\r\n              <div\r\n                class=\"option-btn\"\r\n                v-bind:value=\"12\"\r\n                @click=\"itemsPerPage2 = '12'\"\r\n              >\r\n                12\r\n              </div>\r\n              <div\r\n                class=\"option-btn\"\r\n                v-bind:value=\"18\"\r\n                @click=\"itemsPerPage2 = '18'\"\r\n              >\r\n                18\r\n              </div>\r\n              <div\r\n                class=\"option-btn\"\r\n                v-bind:value=\"24\"\r\n                @click=\"itemsPerPage2 = '24'\"\r\n              >\r\n                24\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!---\r\n          <div class=\"buscador dflx\">\r\n            <input\r\n              v-model=\"search_value\"\r\n              type=\"search\"\r\n              id=\"input-search\"\r\n              class=\"form-control\"\r\n              :placeholder=\"'Búsqueda'\"\r\n            />\r\n            <div class=\"icon_buscar\">\r\n              <img\r\n                @click=\"search(search_value)\"\r\n                src=\"../../assets/default/Search.png\"\r\n                alt=\"icono buscar\"\r\n              />\r\n            </div>\r\n          </div>--->\r\n        </div>\r\n\r\n        <div class=\"dflx widthbtn\">\r\n          <div @click=\"changeView('Add', 'banner-1')\" class=\"btn_añadir dflx\"\r\n           v-if=\"databanneruno.length == 0 && databanneruno == '' && databanneruno !== 'No se encontraron coincidencias' && databanneruno !== 'error'\">\r\n            <p>AGREGAR</p>\r\n            <img src=\"../../assets/default/add.png\" alt=\"\" />\r\n          </div>\r\n\r\n           \r\n        </div>\r\n\r\n        <div class=\"tbl_categorias\">\r\n          <div class=\"tbl_header dflx\">\r\n            <div class=\"tblh tblrad\">\r\n              <p>IMAGEN PC</p>\r\n            </div>\r\n            <div class=\"tblh\">\r\n              <p>IMAGEN MOVIL</p>\r\n            </div>\r\n            <div class=\"tblh\">\r\n              <p>LINK</p>\r\n            </div>\r\n\r\n            \r\n            <div class=\"tblh2 tblh2r\">\r\n              <p>BORRAR</p>\r\n            </div>\r\n          </div>\r\n          <div tbl_content>\r\n            <div\r\n              v-if=\"\r\n                databanneruno != 'No se encontraron coincidencias' &&\r\n                databanneruno != 'error' &&\r\n                databanneruno != ''\r\n              \"\r\n            >\r\n              <div\r\n                class=\"tbl_items dflx\"\r\n                v-for=\"(item, key) in filteredItems2\"\r\n                :key=\"key\"\r\n              >\r\n                <div class=\"tbl_item\">\r\n                  <img\r\n                    :src=\"\r\n                      url   +\r\n                      '/get-banner-image/' +\r\n                      item.image\r\n                    \"\r\n                    alt=\"image\"\r\n                  />\r\n                </div>\r\n                <div class=\"tbl_item\">\r\n                  <img\r\n                    :src=\"\r\n                      url +\r\n                      '/get-banner-image/' +\r\n                      item.image_mv\r\n                    \"\r\n                    alt=\"image\"\r\n                  />\r\n                </div>\r\n                <div class=\"tbl_item\">\r\n                  <p>{{ item.link }}</p>\r\n                </div> \r\n\r\n                <div class=\"icon_eliminar\">\r\n                  <img\r\n                    @click=\"showModal(item.id)\"\r\n                    src=\"../../assets/default/i_eliminar.png\"\r\n                    alt=\"icono eliminar\"\r\n                  />\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else>\r\n              <p class=\"snu\">Aun no hay registros.</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div id=\"navigate-buttons\" class=\"mlf\">\r\n          <div class=\"dflx\">\r\n            <button @click=\"prev2\" id=\"btn-prev\" class=\"btn-navigate\">\r\n              <img src=\"../../assets/default/Left.png\" alt=\"icono anterior\" />\r\n            </button>\r\n            <p class=\"num-pages\">{{ currentPage2 }} / {{ totalPages2 }}</p>\r\n            <button @click=\"next2\" id=\"btn-next\" class=\"btn-navigate\">\r\n              <img src=\"../../assets/default/Right.png\" alt=\"icono siguiente\" />\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </section>\r\n\r\n      <!---BANNER 2-->\r\n      <section>\r\n        <h3 class=\"tlpr\">BANNER 2</h3>\r\n        <div class=\"dflx\">\r\n          <p class=\"noitems\">\r\n            No. de items\r\n            <span v-if=\"databannerdos != 'No se encontraron coincidencias'\">{{\r\n              databannerdos.length\r\n            }}</span>\r\n            <span v-else>0</span>\r\n          </p>\r\n          <div class=\"select-container\">\r\n            <input\r\n              type=\"checkbox\"\r\n              name=\"select-ch\"\r\n              id=\"select-ch\"\r\n              v-model=\"active\"\r\n              true-value=\"true\"\r\n              false-value=\"false\"\r\n            />\r\n            <label for=\"select-ch\">\r\n              <div id=\"select-items\" :class=\"{ rounded: active }\">\r\n                Items por página: {{ itemsPerPage3 }}\r\n              </div>\r\n            </label>\r\n            <div class=\"options-cont\" @click=\"closeSelect()\">\r\n              <div\r\n                class=\"option-btn\"\r\n                v-bind:value=\"6\"\r\n                @click=\"itemsPerPage3 = '6'\"\r\n              >\r\n                6\r\n              </div>\r\n              <div\r\n                class=\"option-btn\"\r\n                v-bind:value=\"12\"\r\n                @click=\"itemsPerPage3 = '12'\"\r\n              >\r\n                12\r\n              </div>\r\n              <div\r\n                class=\"option-btn\"\r\n                v-bind:value=\"18\"\r\n                @click=\"itemsPerPage3 = '18'\"\r\n              >\r\n                18\r\n              </div>\r\n              <div\r\n                class=\"option-btn\"\r\n                v-bind:value=\"24\"\r\n                @click=\"itemsPerPage3 = '24'\"\r\n              >\r\n                24\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!---\r\n          <div class=\"buscador dflx\">\r\n            <input\r\n             