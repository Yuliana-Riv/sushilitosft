{"ast":null,"code":"import dayjs from \"dayjs\";\nimport { mapActions } from \"vuex\";\nexport default {\n  name: \"Suscripciones\",\n  data() {\n    return {\n      status: \"\",\n      message: \"\",\n      item: \"\",\n      search_value: \"\",\n      updsearchcli: \"\",\n      currentPage: 1,\n      itemsPerPage: 12,\n      active: false,\n      viewModal: false,\n      itemDel: \"\",\n      radius: false\n    };\n  },\n  created() {\n    this.getAllInfoSbs(\"suscripciones\");\n  },\n  updated() {\n    if (this.search_value != this.updsearchcli) {\n      this.updsearchcli = this.search_value;\n      let option = \"suscripciones\";\n      let value = this.search_value;\n      let payload = {\n        option: option,\n        search: value\n      };\n      if (value != \"\") {\n        this.$store.dispatch(\"suscripciones/searchSbs\", payload);\n      } else {\n        this.getAllInfoSbs(\"suscripciones\");\n      }\n    }\n  },\n  computed: {\n    data() {\n      return this.$store.getters[\"suscripciones/data\"];\n    },\n    historyOption() {\n      return this.$store.getters[\"suscripciones/getHistoryOption\"];\n    },\n    //Paginacion\n    allItems() {\n      return Object.keys(this.data).map(pid => this.data[pid]);\n    },\n    filteredItems() {\n      return this.allItems.slice((this.currentPage - 1) * this.itemsPerPage, this.currentPage * this.itemsPerPage);\n    },\n    totalPages() {\n      let total = Math.ceil(this.allItems.length / this.itemsPerPage);\n      if (total < this.currentPage) {\n        this.currentPage = 1;\n      }\n      return total;\n    }\n  },\n  methods: {\n    ...mapActions(\"suscripciones\", [\"getAllInfoSbs\"]),\n    ...mapActions(\"suscripciones\", [\"setHistoryOptionSbs\"]),\n    ...mapActions(\"suscripciones\", [\"setAddedSbs\"]),\n    changeView(option, item) {\n      this.item = item;\n      this.setAddedSbs(item);\n      this.setHistoryOptionSbs(option);\n    },\n    activeRadius() {\n      this.radius ? this.radius = false : this.radius = true;\n    },\n    showModal: function (id) {\n      this.itemDel = id;\n      this.viewModal = true;\n    },\n    closeModal: function () {\n      this.viewModal = false;\n      this.itemDel = \"\";\n    },\n    wait: function () {\n      setTimeout(() => this.$router.go(), 200);\n    },\n    delStatus: function () {\n      setTimeout(() => this.delMsgs(), 2000);\n      setTimeout(() => this.wait(), 500);\n    },\n    delMsgs: function () {\n      this.status = \"\";\n      this.message = \"\";\n    },\n    eliminar: async function () {\n      let id = this.itemDel;\n      this.status = \"\";\n      this.message = \"\";\n      let result = await this.$store.dispatch(\"suscripciones/deleteItemSbs\", {\n        option: \"suscripciones\",\n        id: id\n      });\n      if (result.status == \"error\") {\n        this.message = result.message;\n        this.status = \"error\";\n        this.delStatus();\n      } else {\n        this.status = \"\";\n        this.message = \"\";\n        this.wait();\n      }\n    },\n    search: async function (value) {\n      let option = \"suscripciones\";\n      let payload = {\n        option: option,\n        search: value\n      };\n      if (value != \"\") {\n        this.$store.dispatch(\"suscripciones/searchSbs\", payload);\n      } else {\n        this.getAllInfoSbs(\"suscripciones\");\n      }\n    },\n    // botones de paginacion.\n    next() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prev() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    }\n  },\n  filters: {\n    formatDate(date) {\n      return dayjs(date).format(\"DD/MM/YYYY\");\n    }\n  }\n};","map":{"version":3,"names":["dayjs","mapActions","name","data","status","message","item","search_value","updsearchcli","currentPage","itemsPerPage","active","viewModal","itemDel","radius","created","getAllInfoSbs","updated","option","value","payload","search","$store","