        linkC = 'tel:+' + phone;\n      } else if (type == 'CELULAR') {\n        linkC = 'tel:+52' + phone;\n      }\n      let result = await this.$store.dispatch(\"phone/addItemPho\", {\n        option: \"phone\",\n        item: {\n          phone,\n          type,\n          link: linkC\n        }\n      });\n      if (result.status == \"error\") {\n        this.status = \"error\";\n        this.message = result.message;\n      } else {\n        // success\n        this.showModal();\n      }\n    },\n    delStatus: function () {\n      setTimeout(() => this.delMsgs(), 2000);\n    },\n    delMsgs: function () {\n      this.status = \"\";\n      this.message = \"\";\n    },\n    showModal: function () {\n      this.viewModal = true;\n    },\n    Add: function () {\n      this.status = \"\";\n      this.message = \"\";\n      this.phone = \"\";\n      this.type = \"\";\n      this.link = \"\";\n      this.viewModal = false;\n      this.setAddedPho(\"\");\n    },\n    toEdit: function () {\n      this.setHistoryOptionPho(\"Edit\");\n      this.viewModal = false;\n    },\n    back: function () {\n      setTimeout(() => this.$router.go(), 200);\n    }\n  }\n};","map":{"version":3,"names":["mapActions","VueEditor","name","components","data","status","message","phone","type","link","files","viewModal","search_value","computed","identity","$store","getters","methods","previewFiles","$refs","myFiles","addEtiqueta","value","newarr","item","etiquetas","push","etiqueta","delEtiqueta","index","i","length","wait","setTimeout","$router","go","includesItem","search","normalize","replace","toLowerCase","includes","submit","linkC","result","dispatch","option","showModal","delStatus","delMsgs","Add","setAddedPho","toEdit","setHistoryOptionPho","back"],"sources":["src/views/telefono/addPho.vue"],"sourcesContent":["<template>\r\n  <div id=\"AddPho\">\r\n    <div class=\"form-content\">\r\n      <form @submit.prevent=\"submit(phone, type, link)\">\r\n        <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label for=\"titulo\" class=\"titulo\">Teléfono </label>\r\n            <input\r\n              v-model=\"phone\"\r\n              type=\"text\"\r\n              id=\"titulo\"\r\n              name=\"titulo\"\r\n              class=\"wd-lrg\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label for=\"titulo\" class=\"titulo\">Tipo </label>\r\n            <div class=\"dflx\">\r\n              <div\r\n                class=\"selop\"\r\n                :class=\"{ active: type == 'OFICINA' }\"\r\n                @click=\"type = 'OFICINA'\"\r\n              >\r\n                Número Oficina\r\n              </div>\r\n              <div\r\n                class=\"selop\"\r\n                :class=\"{ active: type == 'CELULAR' }\"\r\n                @click=\"type = 'CELULAR'\"\r\n              >\r\n                Número Celular\r\n              </div>\r\n              <div\r\n                class=\"selop\"\r\n                :class=\"{ active: type == 'WHATSAPP' }\"\r\n                @click=\"type = 'WHATSAPP'\"\r\n              >\r\n                Número WhatsApp\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"grup-form dflx\" v-if=\"type == 'WHATSAPP'\">\r\n          <div class=\"boxform\">\r\n            <label for=\"titulo\" class=\"titulo\">Link </label>\r\n            <input\r\n              v-model=\"link\"\r\n              type=\"text\"\r\n              id=\"titulo\"\r\n              name=\"titulo\"\r\n              class=\"wd-lrg\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <!-- <div class=\"grup-form dflx\">\r\n        \r\n        <div class=\"boxform\">\r\n          <label for=\"descripcion\" class=\"descripcion\">Descripción</label>\r\n          <textarea\r\n  